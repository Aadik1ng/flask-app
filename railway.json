{
  "railway": {
    "name": "my-flask-app",
    "command":"python -m spacy download en_core_web_lg" 
              "gunicorn -w 4 -b 0.0.0.0:8080 app:app",
    "env": {
      "FLASK_ENV": "production",
      "SPACY_WARNING_IGNORE": "W008"
    },
    "expose": [
      "8080"
    ],
    "addons": {
      "gunicorn": "latest"
    }
  }
}
